            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "access_code": {
                    "type": "string",
                    "example": "JVXEDFOGOC"
                  },
                  "address1": {
                    "type": "string",
                    "example": ""
                  },
                  "amount": {
                    "type": "string",
                    "example": "100000"
                  },
                  "cancel_url": {
                    "type": "string",
                    "example": "http://sb.ite.com.vn/sand-box/create-order"
                  },
                  "city": {
                    "type": "string",
                    "example": ""
                  },
                  "country": {
                    "type": "string",
                    "example": ""
                  },
                  "currency": {
                    "type": "string",
                    "example": "vnd"
                  },
                  "device": {
                    "type": "string",
                    "example": "os={name=Windows, version=windows-10}, browser={name=Chrome, version=102.0.0.0}, location={long=0, lat=0}"
                  },
                  "email": {
                    "type": "string",
                    "example": ""
                  },
                  "first_name": {
                    "type": "string",
                    "example": ""
                  },
                  "ip_address": {
                    "type": "string",
                    "example": "192.168.1.1"
                  },
                  "issuer_code": {
                    "type": "string",
                    "example": ""
                  },
                  "last_name": {
                    "type": "string",
                    "example": ""
                  },
                  "mac": {
                    "type": "string",
                    "example": "29E998FE8D4859A354723A8A4D68A607"
                  },
                  "mac_type": {
                    "type": "string",
                    "example": "MD5"
                  },
                  "merchant_id": {
                    "type": "string",
                    "example": "102321"
                  },
                  "order_info": {
                    "type": "string",
                    "example": "thanh toán thử"
                  },
                  "order_reference": {
                    "type": "string",
                    "example": "2022061779134309641"
                  },
                  "pay_type": {
                    "type": "string",
                    "example": "pay"
                  },
                  "phone": {
                    "type": "string",
                    "example": ""
                  },
                  "return_url": {
                    "type": "string",
                    "example": "http://sb.ite.com.vn/sand-box/create-order"
                  },
                  "token": {
                    "type": "string",
                    "example": ""
                  },
                  "user_id": {
                    "type": "string",
                    "example": ""
