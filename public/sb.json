                    "message": {
                      "type": "string",
                      "example": "Success"
                    },
                    "error_code": {
                      "type": "string",
                      "example": "00"
                    },
                    "mac": {
                      "type": "string",
                      "example": "FAEFFB991E49CA2288385B326C4FA9AC"
                    },
                    "mac_type": {
                      "type": "string",
                      "example": "MD5"
                    }
                  }
                }
              }
            }
          },
          "201": {
            "description": "Created"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
