                    "session_id": {
                      "type": "string",
                      "example": "abdc47da4336699c2d5f380bac493c63"
                    },
                    "merchant_id": {
                      "type": "string",
                      "example": "102321"
                    },
                    "amount": {
                      "type": "string",
                      "example": "100000.00"
                    },
                    "currency": {
                      "type": "string",
                      "example": "vnd"
                    },
                    "expire_time": {
                      "type": "string",
                      "example": "17-06-2022 11:42:29"
                    },
                    "order_reference": {
                      "type": "string",
                      "example": "2022061779134309641"
                    },
                    "payment_url": {
                      "type": "string",
