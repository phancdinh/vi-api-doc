    "paths": {
        "/paygate/get-order": {
            "post": {
                "tags": [
                    "Common"
                ],
                "summary": "Lấy thông tin đơn hàng",
                "operationId": "paygate-get-order",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "check_sum": {
                                        "type": "string",
                                        "example": "nu/2MAuTrJrQAEQMKunmCJfClW8="
                                    },
                                    "device": {
                                        "type": "string",
                                        "example": "os={name=Windows, version=windows-10}, browser={name=Chrome, version=102.0.0.0}, location={long=0, lat=0}"
                                    },
                                    "ip_address": {
                                        "type": "string",
                                        "example": "192.168.1.1"
                                    },
                                    "mac": {
                                        "type": "string",
                                        "example": "4594228DDDF777AE58EBF34A09B20C78"
                                    },
                                    "mac_type": {
                                        "type": "string",
                                        "example": "MD5"
                                    },
                                    "session_id": {
                                        "type": "string",
                                        "example": "4ae53d6d05725793d1eceb1813b5fcc1"
                                    }
                                },
                                "required": [
                                    "device",
                                    "ip_address",
                                    "mac_type", "mac", "session_id"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "merchant_id": {
                                            "type": "string",
                                            "example": "102321"
                                        },
                                        "amount": {
                                            "type": "integer",
                                            "format": "int32",
                                            "example": "100000"
                                        },
                                        "order_info": {
                                            "type": "string",
                                            "example": "thanh toán thử"
                                        },
                                        "order_reference": {
                                            "type": "string",
                                            "example": "2022061746635906497"
                                        },
                                        "session_id": {
                                            "type": "string",
                                            "example": "4ae53d6d05725793d1eceb1813b5fcc1"
                                        },
                                        "qr_url": {
                                            "type": "string",
                                            "example": "https://paygate-apidev.ite.com.vn/ite-pg-qr/view/1655448734308173751"
                                        },
                                        "currency": {
                                            "type": "string",
                                            "example": "vnd"
                                        },
                                        "cancle_url": {
                                            "type": "string",
                                            "example": "http://sb.ite.com.vn/sand-box/create-order"
                                        },
                                        "return_url": {
                                            "type": "string",
                                            "example": "http://sb.ite.com.vn/sand-box/create-order?error_code=14&pg_amount=100000.00&pg_currency=vnd&pg_merchant_id=102321&pg_order_info=thanh toán thử&pg_order_reference=2022061746635906497&pg_payment_method=0&session_id=4ae53d6d05725793d1eceb1813b5fcc1&mac_type=SHA256&mac=910C3AEF6AF3C1D240CEAD778B4B7C967D0EA06B8FB52B813BC9EA332B6EEC62"
                                        },
                                        "pay_type": {
                                            "type": "string",
                                            "example": "pay"
                                        },
                                        "expired_time": {
                                            "type": "string",
                                            "example": "2022-06-17 14:07:14"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Success"
                                        },
                                        "error_code": {
                                            "type": "string",
                                            "example": "00"
                                        },
                                        "merchant_name": {
                                            "type": "string",
                                            "example": "ABCS"
                                        },
                                        "payment_method": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "format": "int32",
                                                        "example": "320"
                                                    },
                                                    "name": {
                                                        "type": "string",
                                                        "example": "Ví điện tử"
                                                    },
                                                    "logo": {
                                                        "type": "string",
                                                        "example": "http://103.57.211.14:11932/vtl-pg/mm/media/payment_method/logo/ea63543c744889f24edcc96df95e2c04.jpg"
                                                    },
                                                    "index": {
                                                        "type": "integer",
                                                        "format": "int32",
                                                        "example": "1"
                                                    },
                                                    "issuer": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "object",
                                                            "properties": {
                                                                "code": {
                                                                    "type": "string",
                                                                    "example": "970422"
                                                                },
                                                                "bin": {
                                                                    "type": "array",
                                                                    "items": {
                                                                        "type": "string",
                                                                        "example": "970422"
                                                                    }
                                                                },
                                                                "bizname": {
                                                                    "type": "string",
                                                                    "format": "nullable"
                                                                },
                                                                "verify_payment_info_step": {
                                                                    "type": "string",
                                                                    "example": "internal"
                                                                },
                                                                "logo": {
                                                                    "type": "string",
                                                                    "example": "https://paygate-apidev.ite.com.vn/ite-pg/mm/no-auth/media/issuer/logo/bffd77603398737bc10fec92eb86aa2a"
                                                                },
                                                                "issuer_params": {
                                                                    "type": "object",
                                                                    "properties": {
                                                                        "hint": {
                                                                            "type": "string",
                                                                            "example": "MBB"
                                                                        },
                                                                        "params": {
                                                                            "type": "array",
                                                                            "items": {
                                                                                "type": "object",
                                                                                "properties": {
                                                                                    "holder_name": {
                                                                                        "type": "string",
                                                                                        "example": "holder_name_label"
                                                                                    },
                                                                                    "require": {
                                                                                        "type": "integer",
                                                                                        "format": "int32",
                                                                                        "example": "1"
                                                                                    }
                                                                                }
                                                                            }
                                                                        }
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "201": {
                        "description": "Created"
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
